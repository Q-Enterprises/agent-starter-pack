{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": {}, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "id": 1,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum(rate(vector_validation_runs_total{result=\"success\",job=~\"$job\",collection=~\"$collection\"}[5m])) / clamp_min(sum(rate(vector_validation_runs_total{job=~\"$job\",collection=~\"$collection\"}[5m])), 1e-9)",
          "legendFormat": "success_rate",
          "refId": "A"
        }
      ],
      "title": "Validation Success Rate (5m)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": {}, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "id": 2,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum by (drift_class) (rate(vector_validation_drift_events_total{job=~\"$job\",collection=~\"$collection\"}[5m]))",
          "legendFormat": "{{drift_class}}",
          "refId": "A"
        }
      ],
      "title": "Drift Events Rate by Class (5m)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": {}, "overrides": [] },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 8 },
      "id": 3,
      "targets": [
        {
          "expr": "sum(rate(vector_validation_points_sampled_total{job=~\"$job\",collection=~\"$collection\"}[5m]))",
          "legendFormat": "points_sampled_per_sec",
          "refId": "A"
        }
      ],
      "title": "Sampling Throughput (points/sec)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": {}, "overrides": [] },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 8 },
      "id": 4,
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(vector_validation_run_duration_seconds_bucket{job=~\"$job\",collection=~\"$collection\"}[5m])))",
          "legendFormat": "p95_duration_s",
          "refId": "A"
        }
      ],
      "title": "Run Duration p95 (5m)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 15 },
      "id": 5,
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"], "fields": "" },
        "orientation": "horizontal",
        "textMode": "value"
      },
      "targets": [
        {
          "expr": "time() - max(vector_validation_last_success_timestamp_seconds{job=~\"$job\",collection=~\"$collection\"})",
          "legendFormat": "seconds_since_last_success",
          "refId": "A"
        }
      ],
      "title": "Seconds Since Last Success",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 15 },
      "id": 6,
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"], "fields": "" },
        "orientation": "horizontal",
        "textMode": "value"
      },
      "targets": [
        {
          "expr": "sum(increase(vector_validation_drift_events_total{job=~\"$job\",collection=~\"$collection\"}[1h]))",
          "legendFormat": "drift_events_1h",
          "refId": "A"
        }
      ],
      "title": "Total Drift Events (1h)",
      "type": "stat"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["vector-validation", "drift", "qdrant"],
  "templating": {
    "list": [
      {
        "current": { "selected": false, "text": "Prometheus", "value": "PROM" },
        "hide": 2,
        "name": "DS_PROMETHEUS",
        "options": [],
        "query": "prometheus",
        "type": "datasource"
      },
      {
        "name": "job",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": { "query": "label_values(vector_validation_runs_total, job)" },
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": { "selected": true, "text": "All", "value": "$__all" }
      },
      {
        "name": "collection",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": { "query": "label_values(vector_validation_runs_total{job=~\"$job\"}, collection)" },
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": { "selected": true, "text": "All", "value": "$__all" }
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Vector Store Integrity â€” Drift & Validation",
  "version": 1
}
